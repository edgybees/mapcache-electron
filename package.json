    "lodash": "4.17.20",
    "mime": "^2.5.2",
    "mkdirp": "1.0.4",
    "moment": "2.29.1",
    "ol": "6.5.0",
    "pako": "2.0.3",
    "pbf": "3.2.1",
    "proj4": "2.6.2",
    "reproject": "1.2.6",
    "shpjs": "3.6.3",
    "typeface-roboto": "1.1.13",
    "uuid": "8.3.1",
