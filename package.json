{
  "name": "mapcache",
  "version": "1.0.8",
  "description": "Desktop Electron app to create GeoPackages from geospatial data sources.",
  "author": "Dan Barela <daniel.barela@gmail.com>",
  "mapcache": {
    "store": {
      "version": "7"
    }
  },
  "scripts": {
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:build-mac": "vue-cli-service electron:build -- --mac --x64",
    "electron:build-win": "vue-cli-service electron:build -- --win portable nsis",
    "electron:build-linux": "vue-cli-service electron:build --linux",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "electron:serve": "node --max_old_space_size=8192 node_modules/.bin/vue-cli-service electron:serve",
    "electron:debug": "node --max_old_space_size=8192 node_modules/.bin/vue-cli-service electron:serve --debug; electron --inspect=5858 dist_electron",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@ccaldwell/togeojson": "git://github.com/caldwellc/togeojson.git#v3.0.4",
    "@esri/arcgis-to-geojson-utils": "1.3.0",
    "@jimp/core": "^0.16.1",
    "@jimp/custom": "^0.16.1",
    "@jimp/plugin-rotate": "^0.16.1",
    "@jimp/types": "^0.16.1",
    "@ngageoint/geopackage": "4.0.0-beta.21",
    "@turf/bbox": "6.3.0",
    "@turf/bbox-polygon": "6.3.0",
    "@turf/intersect": "6.3.0",
    "@turf/transform-rotate": "6.3.0",
    "adm-zip": "0.5.1",
    "axios": "0.21.1",
    "better-sqlite3": "7.1.4",
    "bindings": "git://github.com/caldwellc/node-bindings.git#master",
    "canvas": "2.7.0",
    "electron-dl": "3.0.2",
    "electron-log": "4.3.0",
    "file-url": "3.0.0",
    "fs-jetpack": "4.0.1",
    "geotiff": "1.0.4",
    "image-size": "0.9.3",
    "imagemin": "7.0.1",
    "imagemin-pngquant": "git://github.com/caldwellc/imagemin-pngquant.git#master",
    "leaflet": "1.7.1",
    "leaflet-draw": "1.0.4",
    "leaflet-editable": "1.2.0",
