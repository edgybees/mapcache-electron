{
  "name": "mapcache",
  "version": "1.0.0",
  "author": "Dan Barela <daniel.barela@gmail.com>",
  "contributors": [
    {
      "name": "Christopher Caldwel",
      "email": "c.caldwell90@gmail.com"
    },
    {
      "name": "Joe Clark",
      "email": "jclark118@gmail.com"
    }
  ],
  "description": "Desktop Electron app to create GeoPackages from geospatial data sources.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngageoint/mapcache-electron.git"
  },
  "main": "./dist/electron/main.js",
  "scripts": {
    "cleanInstall": "rm -rf node_modules",
    "copyResources": "mkdir -p build && cp -R buildResources/* build",
    "build": "npm run copyResources && node .electron-vue/build.js && electron-builder",
    "build-windows": "npm run copyResources && node .electron-vue/build.js && electron-builder --win portable nsis",
    "build-mac": "npm run copyResources && node --max-old-space-size=8192 .electron-vue/build.js && electron-builder --mac",
    "build-linux": "npm run copyResources && node .electron-vue/build.js && electron-builder --linux",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test-old": "npm run unit && npm run e2e",
    "unitold": "karma start test/unit/karma.conf.js",
    "unit": "cross-env NODE_ENV=test nyc --no-clean mocha",
    "test": "npm run clean-test && npm run unit && npm run report",
    "report": "nyc report",
    "clean-test": "rm -rf ./.test_run; rm -rf ./.nyc_output; rm -rf ./docs/coverage",
    "rebuild": "electron-rebuild -w better-sqlite3 gdal canvas integer",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "temp-dir": "./.test_run",
    "report-dir": "./docs/coverage",
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test-e2e/**/*",
      "test-vue/**/*",
      "dist/**/*",
      "docs/**/*",
      "vendor/**/*",
      "test/**/*"
    ],
    "extension": [
      ".js"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "build": {
    "productName": "MapCache",
    "appId": "mil.nga.mapcache",
    "afterSign": "build/notarize.js",
    "copyright": "Copyright Â© 2020 National Geospatial-Intelligence Agenecy",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "sign": false
    },
    "mas": {
      "entitlements": "build/entitlements.mas.plist",
      "entitlementsInherit": "build/entitlements.mas.inherit.plist",
      "binaries": [
        "build/mas/MapCache.app/Contents/Resources/app/node_modules/better-sqlite3/build/Release/better_sqlite3.node",
        "build/mas/MapCache.app/Contents/Resources/app/node_modules/canvas/build/Release/canvas.node",
        "build/mas/MapCache.app/Contents/Resources/app/node_modules/gdal/build/Release/gdal.node",
        "build/mas/MapCache.app/Contents/Resources/app/node_modules/integer/build/Release/integer.node"
      ],
      "provisioningProfile": "/Users/caldwell/Downloads/MapCache_MacOS_Development.provisionprofile",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "pkg"
      ],
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "build/icons/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "icon": "build/icons",
      "target": [
        "deb",
        "rpm",
        "tar.gz"
      ]
    }
  },
  "dependencies": {
    "@ccaldwell/togeojson": "git://github.com/caldwellc/togeojson.git#v3.0.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
